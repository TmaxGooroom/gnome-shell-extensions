+    selected(fileItem, action) {
+        switch(action) {
+        case Enums.Selection.ALONE:
+            if (!fileItem.isSelected) {
+                for(let item of this._fileList) {
+                    if (item === fileItem) {
+                        item.setSelected();
+                    } else {
+                        item.unsetSelected();
