+        this._volumeMonitor.connect('mount-added', () => { this._updateDesktop() });
+        this._volumeMonitor.connect('mount-removed', () => { this._updateDesktop() });
+
+        this.rubberBand = false;
+
+        let cssProvider = new Gtk.CssProvider();
