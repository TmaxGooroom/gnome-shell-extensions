+            if (grid !== null) {
+                xDestination = grid[0];
+                yDestination = grid[1];
+                break;
+            }
+        }
+        let deltaX = xDestination - xOrigin;
+        let deltaY = yDestination - yOrigin;
+        let fileItems = [];
+        for(let item of this._fileList) {
+            if (item.isSelected) {
