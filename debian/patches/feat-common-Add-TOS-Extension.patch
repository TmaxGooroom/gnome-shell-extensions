+        case Enums.Selection.RIGHT_BUTTON:
+            if (!fileItem.isSelected) {
+                for(let item of this._fileList) {
+                    if (item === fileItem) {
+                        item.setSelected();
+                    } else {
+                        item.unsetSelected();
+                    }
+                }
+            }
+            break;
+        case Enums.Selection.ENTER:
+            if (this.rubberBand) {
+                fileItem.setSelected();
+            }
+            break;
+        case Enums.Selection.RELEASE:
+            for(let item of this._fileList) {
+                if (item === fileItem) {
+                    item.setSelected();
