+    let current_workspace = Utils.DisplayWrapper.getWorkspaceManager().get_active_workspace();
+    for (let i = 0; i < windows.length; i++) {
+        let w = windows[i];
+        if (w.get_workspace() == current_workspace && w.showing_on_its_workspace()) {
+            w.minimize();
