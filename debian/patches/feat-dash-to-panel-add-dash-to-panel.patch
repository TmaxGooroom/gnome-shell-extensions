+            {
+                app,
+                window,
+                isLauncher,
+            },
+            this.dtpPanel,
+            {
+                setSizeManually: true,
+                showLabel: false,
+                isDraggable: !Me.settings.get_boolean('taskbar-locked'),
+            },
+            this.previewMenu
+        );
