+        // styles for theming
+        Object.keys(St.Side).forEach(p => {
+            let cssName = `dashtopanel${p.charAt(0)}${p.slice(1).toLowerCase()}`;
+
+            this.panel.actor[`${St.Side[p] == this.geom.position ? 'add' : 'remove'}_style_class_name`](cssName);
