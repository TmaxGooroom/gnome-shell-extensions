+            this._updateWorkspacesFullGeometry();
+            this._updateWorkspacesActualGeometry();
+        }
+    },
+
+    _newGetShowAppsButton() {
+        let focusedMonitorIndex = Utils.findIndex(this.allPanels, p => this.checkIfFocusedMonitor(p.monitor));
