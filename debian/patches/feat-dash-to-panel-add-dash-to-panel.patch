+        if (appIcon._draggable) {
+            appIcon._draggable.connect('drag-begin',
+                Lang.bind(this, () => {
+                    appIcon.actor.opacity = 50;
+                }));
+            appIcon._draggable.connect('drag-end',
+                Lang.bind(this, () => {
+                    appIcon.actor.opacity = 255;
+                }));
