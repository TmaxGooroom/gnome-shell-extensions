+                Mainloop.source_remove(this._ensureAppIconVisibilityTimeoutId);
+                this._ensureAppIconVisibilityTimeoutId = 0;
+            }
+        }));
+
+        appIcon.actor.connect('clicked',
+            Lang.bind(this, function (actor) {
