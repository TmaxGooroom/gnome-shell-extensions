+        _set(actor, false);
+        _set(actor, isVertical);
+    },
+
+    _disconnectVisibleId(actor) {
+        actor.disconnect(actor._dtpVisibleId);
+        actor.disconnect(actor._dtpDestroyId);
+
+        delete actor._dtpVisibleId;
+        delete actor._dtpDestroyId;
